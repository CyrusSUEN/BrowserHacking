{
  "manifest_version": 2,

  "name": "Redaction Example",
  "description": "Redact all text, everywhere",
  "version": "5.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "jquery-2.2.3.js", "content.js" ]
  }],

  "web_accessible_resources": [
    "fonts/*"
  ],

  "permissions": [
    "activeTab",
    "http://*/"
  ]
}

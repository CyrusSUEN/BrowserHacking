{
  "manifest_version": 2,

  "name": "Content Script Test",
  "description": "Check if a page has blockable content",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [ "jquery-2.2.3.js", "content.js" ]
  }],

  "permissions": [
    "activeTab",
    "http://*/"
  ]
}
